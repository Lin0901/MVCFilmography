@model List<Movie>

    <h1>@ViewData["Title"]</h1>

@foreach (Movie m in @Model)
{
    <h2>@m.Title</h2>
    <div>@m.Category</div>
    <div>￥@m.Budget</div>
    <div>@m.Date.Year</div>
}

